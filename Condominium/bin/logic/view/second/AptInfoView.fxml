<AnchorPane stylesheets="@style.css" styleClass="AptInfo" style="-fx-font-size: 20" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="logic.controller.guicontroller.second.resident.AptInfoGUI">
    <VBox fx:id="gridVbox" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="20.0">
        <GridPane fx:id="feeGrid">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="300.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
